		<defName>MB_DarknessBolt</defName>
		<label>darkness bolt</label>
		<description>Placeholder</description>
		<iconPath>UI/Abilities/Ability_DarknessBolt_Icon</iconPath>
		<cooldownTicksRange>60</cooldownTicksRange>
